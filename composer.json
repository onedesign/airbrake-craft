{
    "name": "onedesign/airbrake-craft",
    "description": "Logs errors to airbrake.io",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "airbrake"
    ],
    "support": {
        "docs": "https://github.com/onedesign/airbrake/blob/master/README.md",
        "issues": "https://github.com/onedesign/airbrake/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "One Design Company",
            "homepage": "https://onedesigncompany.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "airbrake/phpbrake": "^0.6.0"
    },
    "autoload": {
        "psr-4": {
          "onedesign\\airbrake\\": "src/"
        }
    },
    "extra": {
        "name": "Airbrake",
        "handle": "airbrake",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/onedesign/airbrake/master/CHANGELOG.md",
        "class": "onedesign\\airbrake\\Airbrake"
    }
}
